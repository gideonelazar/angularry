
{
    "npmPublish": true,
    "publish": [
    {
        "path": "@semantic-release/github"
    },
    {
        "path": "@semantic-release/exec",
        "cmd": "echo ${nextRelease.version}"
    }
],
    "analyzeCommits": {
    "preset": "angular",
        "releaseRules": [
        {
            "type": "docs",
            "scope": "README",
            "release": "patch"
        },
        {
            "type": "refactor",
            "release": "patch"
        },
        {
            "type": "style",
            "release": "patch"
        }
    ],
        "parserOpts": {
        "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING"
        ]
    }
},
    "git": {
    "assets": ["dist/**/*.{js,css}", "docs", "package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
},
    "notesGenerator": {
    "preset": "angular",
        "parserOpts": {
        "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING"
        ]
    },
    "writerOpts": {
        "commitsSort": [
            "subject",
            "scope"
        ]
    }
}
}
